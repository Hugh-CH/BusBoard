@model List<List<PredictionViewModel>>

@{
    ViewBag.Title = "Arrivals with Postcode";
    Layout = "_Layout";
}
@foreach (var station in Model)
{
    <h2>Next Buses at @station[0].stationName </h2>
    <div>
        <ol>
            @foreach (var arrival in station)
            {
                <li>@arrival.GetString()</li>
            }
        </ol>
    </div>
}